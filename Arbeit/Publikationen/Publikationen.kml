<Document>
	<name>Publikationen</name>
	<open>1</open>

	
		<Style id="Mysterien">
		<ListStyle>
		  	<ItemIcon>
				<href>http://www.dereglobus.orkenspalter.com/public/DereGlobus/Publikationen/media/icons/Mysterium.png</href>	
			</ItemIcon>
			<maxSnippetLines>0</maxSnippetLines>
		</ListStyle>
		<BalloonStyle>
			<!-- a background color for the balloon -->
			<bgColor>FF1C1A1A</bgColor>
			<!-- styling of the balloon text -->
			<text>
				<![CDATA[
				<font face="Calibri" color="#366698" size="+3">$[name]</font>
				<br/><br/>
				<font color="#bbbbbb">$[description]</font>
				]]>
			</text>
			<textColor>FF000000</textColor>
      </BalloonStyle>
	</Style>
	
	
