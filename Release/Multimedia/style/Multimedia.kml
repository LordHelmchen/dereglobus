	<Style id="Musik-n">
		<ListStyle>
			<ItemIcon>
				<href>http://dereglobus.orkenspalter.de/public/DereGlobus/Multimedia/media/icons/Musik.png</href>	
			</ItemIcon>
			<maxSnippetLines>0</maxSnippetLines>
		</ListStyle>
		<LabelStyle>
			<scale>0</scale>
			<color>00ffffff</color>
		</LabelStyle>

		<IconStyle>
			<scale>0.9</scale>
			<color>ffffffff</color>
			<colorMode>normal</colorMode>
			<Icon>
				<href>http://dereglobus.orkenspalter.de/public/DereGlobus/Multimedia/media/icons/Musik.png</href>
			</Icon>
			<hotSpot x="0.5" y="0" xunits="fraction" yunits="fraction"/>
		</IconStyle>
		<BalloonStyle>
			<!-- a background color for the balloon -->
			<bgColor>FF5999C8</bgColor>
			<!-- styling of the balloon text -->
			<text>
				<![CDATA[
				<font face="Mason" color="#B21C19" size="+3">$[name]</font>
				<br/><br/>
				<font color="#000000">$[description]</font>
				]]>
			</text>
			<textColor>FF000000</textColor>
		</BalloonStyle>
	</Style>



