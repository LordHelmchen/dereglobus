        <name>Die Ungeschlagenen</name>  <ExtendedData><Data name="weltenbauer"><value>DG-Team</value></Data><Data name="weltenbauerURL"><value>http://www.dereglobus.org/team</value></Data><Data name="Link"><value>$[name]</value></Data></ExtendedData>
