      <Placemark>
        <name>Der erste Kaiser</name>  <ExtendedData><Data name="weltenbauer"><value>DG-Team</value></Data><Data name="weltenbauerURL"><value>http://www.dereglobus.org/team</value></Data></ExtendedData>
