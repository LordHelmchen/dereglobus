        <!-- a background color for the balloon -->
        <bgColor>FFD5FCFD</bgColor>
        <!-- styling of the balloon text -->
<text><![CDATA[<font face="Mason" color="#CC0000" size="+3"><a title="Im Wiki-Aventurica öffnen" href="http://www.wiki-aventurica.de/index.php?title=$[Link]">$[name]</a></font>
<br/><br/>
$[description]]]></text>
