<Document>
<name>Kanal-Styles</name>


	<StyleMap id="kanal">
		<Pair>
			<key>normal</key>
			<styleUrl>#kanal-n</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#kanal-h</styleUrl>
		</Pair>
	</StyleMap>


	<StyleMap id="kanalohne">
		<Pair>
			<key>normal</key>
			<styleUrl>#kanalohne-n</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#kanalohne-h</styleUrl>
		</Pair>
	</StyleMap>
	




<Style id="kanal-n">
      <ListStyle>
         <maxSnippetLines>0</maxSnippetLines>
      </ListStyle>
		<LabelStyle>
			<scale>0</scale>
			<color>00ffffff</color>
		</LabelStyle>
		<LineStyle>
			<color>7f7f5500</color>
			<width>3</width>
		</LineStyle>
<BalloonStyle>
<text><![CDATA[<font color="#CC0000" size="+3">$[name]</font>
<br/><br/>
$[description]]]></text>
</BalloonStyle>
	</Style>
	
	<Style id="kanal-h">
    		<ListStyle>
       			  <maxSnippetLines>0</maxSnippetLines>
     		</ListStyle>
		<LineStyle>
			<color>b27f5500</color>
			<width>3</width>
		</LineStyle>
		<LabelStyle>
			<scale>1</scale>
			<color>ffffffff</color>
		</LabelStyle>
		<BalloonStyle>
<text><![CDATA[<font color="#CC0000" size="+3">$[name]</font>
<br/><br/>
$[description]]]></text>
</BalloonStyle>
	</Style>

	
