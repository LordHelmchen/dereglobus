<Document>
<name>Regionen</name>


	<StyleMap id="region">
		<Pair>
			<key>normal</key>
			<styleUrl>#region-n</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#region-h</styleUrl>
		</Pair>
	</StyleMap>


