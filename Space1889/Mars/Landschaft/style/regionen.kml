<Style id="region-n">
		<IconStyle>
			<color>00ffffff</color>
			<scale>0</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/shapes/target.png</href>
			</Icon>
		</IconStyle>
      <ListStyle>
         <maxSnippetLines>0</maxSnippetLines>
      </ListStyle>
		<LabelStyle>
			<color>cc84d9f0</color>
			<scale>2</scale>
		</LabelStyle>
<BalloonStyle>
        <bgColor>FFD5FCFD</bgColor>
<text><![CDATA[<font color="#CC0000" size="+3">$[name]</font>
<br/><br/>
$[description]]]></text>
</BalloonStyle>
	</Style>
	
