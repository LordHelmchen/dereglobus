<Document>
<name>rohstoff-Styles</name>


	<StyleMap id="rohstoff">
		<Pair>
			<key>normal</key>
			<styleUrl>#rohstoff-n</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#rohstoff-h</styleUrl>
		</Pair>
	</StyleMap>





<Style id="rohstoff-n">
      <ListStyle>
         <maxSnippetLines>0</maxSnippetLines>
      </ListStyle>
		<LabelStyle>
			<scale>1</scale>
			<color>ffffffff</color>
		</LabelStyle>
		<LineStyle>
			<color>7f7f5500</color>
			<width>3</width>
		</LineStyle>
		<IconStyle>
			<scale>1.5</scale>
			<Icon>
				<href>http://dereglobus.googlecode.com/svn/Space1889_e/Mars/Landschaft/media/Rohstoff.png</href>
			</Icon>
		</IconStyle>
<BalloonStyle>
        <bgColor>FFD5FCFD</bgColor>
<text><![CDATA[<font color="#CC0000" size="+3">$[name]</font>
<br/><br/>
$[description]]]></text>
</BalloonStyle>
	</Style>
	
	<Style id="rohstoff-h">
    		<ListStyle>
       			  <maxSnippetLines>0</maxSnippetLines>
     		</ListStyle>
		<LineStyle>
			<color>b27f5500</color>
			<width>3</width>
		</LineStyle>
		<LabelStyle>
			<scale>1.5</scale>
			<color>ffffffff</color>
		</LabelStyle>
		<IconStyle>
			<scale>2</scale>
			<Icon>
				<href>http://dereglobus.googlecode.com/svn/Space1889_e/Mars/Landschaft/media/Rohstoff.png</href>
