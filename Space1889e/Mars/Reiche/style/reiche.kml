<BalloonStyle>
        <bgColor>FFD5FCFD</bgColor>
<text><![CDATA[<font color="#CC0000" size="+3">$[name]</font>
<br/><br/>
$[description]]]></text>
</BalloonStyle>
	</Style>

	
</Document>
